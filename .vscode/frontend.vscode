    <script>
        // ==================== PAGE NAVIGATION ====================
        document.addEventListener('DOMContentLoaded', function() {
            const pages = {
                home: document.getElementById('homePage'),
                register: document.getElementById('registerPage'),
                payment: document.getElementById('paymentPage'),
                confirmation: document.getElementById('confirmationPage'),
                committees: document.getElementById('committeesPage'),
                resources: document.getElementById('resourcesPage'),
                about: document.getElementById('aboutPage')
            };
            
            const navLinks = {
                home: document.getElementById('homeLink'),
                agenda: document.getElementById('agendaLink'),
                resources: document.getElementById('resourcesLink'),
                register: document.getElementById('registerLink'),
                about: document.getElementById('aboutLink'),
                homeBtn: document.getElementById('homeBtn'),
                agendaBtn: document.getElementById('agendaBtn'),
                resourcesBtn: document.getElementById('resourcesBtn'),
                registerBtn: document.getElementById('registerBtn'),
                footerAgenda: document.getElementById('footerAgenda'),
                footerResources: document.getElementById('footerResources'),
                footerAbout: document.getElementById('footerAbout'),
                backToHome: document.getElementById('backToHome'),
                backToRegister: document.getElementById('backToRegister'),
                submitPayment: document.getElementById('submitPayment'),
                confirmBackToHome: document.getElementById('confirmBackToHome'),
                downloadBadge: document.getElementById('downloadBadge')
            };
            
            // Function to show a specific page and hide others
            function showPage(pageId) {
                Object.values(pages).forEach(page => {
                    page.classList.remove('active');
                });
                pages[pageId].classList.add('active');
                scrollToTop();
                
                // Special setup for committees and resources pages
                if (pageId === 'committees') {
                    createStars('starsContainer');
                    setupAgendaSelection();
                } else if (pageId === 'resources') {
                    createStars('starsResources');
                    setupResourceSelection();
                } else if (pageId === 'about') {
                    createStars('starsAbout');
                }
            }
            
            // ==================== REGISTRATION FORM ====================
            function validateForm(formData) {
                const email = formData.email;
                const phone = formData.phone;
                
                // Email validation
                const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
                if (!emailRegex.test(email)) {
                    alert('Please enter a valid email address');
                    return false;
                }
                
                // Phone validation (if provided)
                if (phone && !/^\d{10}$/.test(phone)) {
                    alert('Please enter a valid 10-digit phone number');
                    return false;
                }
                
                return true;
            }

            document.getElementById('munRegistrationForm').addEventListener('submit', function(e) {
                e.preventDefault();
                
                // Get form data
                const formData = {
                    fullName: document.getElementById('fullName').value,
                    email: document.getElementById('email').value,
                    phone: document.getElementById('phone').value,
                    school: document.getElementById('school').value,
                    experience: document.getElementById('experience').value,
                    country1: document.getElementById('country1').value,
                    country2: document.getElementById('country2').value,
                    country3: document.getElementById('country3').value,
                    committee: document.getElementById('committee').value
                };
                
                // Validate form
                if (!validateForm(formData)) {
                    return; // Stop if validation fails
                }
                
                // Validate required fields
                if (!formData.fullName || !formData.email || !formData.school || !formData.experience || !formData.country1 || !formData.committee) {
                    alert('Please fill in all required fields.');
                    return;
                }
                
                // Store registration data in localStorage
                localStorage.setItem('registrationData', JSON.stringify(formData));
                
                // Generate a registration ID
                const registrationId = 'MUN-' + Date.now().toString().slice(-6);
                localStorage.setItem('registrationId', registrationId);
                
                // Update payment page with actual data
                updatePaymentDetails();
                
                // Navigate to payment page
                showPage('payment');
            });

            // ==================== PAYMENT HANDLING ====================
            document.getElementById('submitPayment').addEventListener('click', function(e) {
                e.preventDefault();
                
                const transactionId = document.getElementById('transactionId').value;
                const fileInput = document.getElementById('fileInput');
                
                if (!transactionId) {
                    alert('Please enter your transaction ID');
                    return;
                }
                
                if (!fileInput.files[0]) {
                    alert('Please upload payment screenshot');
                    return;
                }

                // Store the entered transaction ID
                localStorage.setItem('transactionId', transactionId);
                
                // Update confirmation page with actual data
                updateConfirmationDetails();
                
                // Navigate to confirmation page
                showPage('confirmation');
            });

            // ==================== UPDATE UI FUNCTIONS ====================
            function updatePaymentDetails() {
                const registrationData = JSON.parse(localStorage.getItem('registrationData') || '{}');
                const registrationId = localStorage.getItem('registrationId');
                
                document.getElementById('delegateName').textContent = registrationData.fullName || 'Not provided';
                document.getElementById('delegateCommittee').textContent = registrationData.committee || 'Not provided';
                document.getElementById('delegateCountry').textContent = registrationData.country1 || 'Not provided';
                document.getElementById('registrationId').textContent = registrationId || 'Not generated';
            }

            function updateConfirmationDetails() {
                const registrationData = JSON.parse(localStorage.getItem('registrationData') || '{}');
                const registrationId = localStorage.getItem('registrationId');
                const transactionId = localStorage.getItem('transactionId');
                
                document.getElementById('confirmRegId').textContent = registrationId || 'Not provided';
                document.getElementById('confirmName').textContent = registrationData.fullName || 'Not provided';
                document.getElementById('confirmCommittee').textContent = registrationData.committee || 'Not provided';
                document.getElementById('confirmCountry').textContent = registrationData.country1 || 'Not provided';
                document.getElementById('confirmTxId').textContent = transactionId || 'Not provided';
            }

            // ==================== FILE UPLOAD HANDLING ====================
            const fileInput = document.getElementById('fileInput');
            const fileName = document.getElementById('fileName');
            const uploadBox = document.getElementById('uploadBox');

            if (fileInput && fileName && uploadBox) {
                fileInput.addEventListener('change', function() {
                    if (this.files[0]) {
                        fileName.textContent = this.files[0].name;
                        uploadBox.style.borderColor = '#4CAF50';
                    }
                });
                
                uploadBox.addEventListener('click', () => fileInput.click());
                
                uploadBox.addEventListener('dragover', (e) => {
                    e.preventDefault();
                    uploadBox.style.borderColor = '#5C6BC0';
                    uploadBox.style.backgroundColor = 'rgba(92, 107, 192, 0.1)';
                });
                
                uploadBox.addEventListener('dragleave', () => {
                    uploadBox.style.borderColor = '#ccc';
                    uploadBox.style.backgroundColor = 'rgba(41, 47, 85, 0.3)';
                });
                
                uploadBox.addEventListener('drop', (e) => {
                    e.preventDefault();
                    uploadBox.style.borderColor = '#ccc';
                    uploadBox.style.backgroundColor = 'rgba(41, 47, 85, 0.3)';
                    
                    if (e.dataTransfer.files[0]) {
                        fileInput.files = e.dataTransfer.files;
                        fileName.textContent = e.dataTransfer.files[0].name;
                        uploadBox.style.borderColor = '#4CAF50';
                    }
                });
            }

