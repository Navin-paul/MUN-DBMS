                // Update agenda content based on selected committee
                function updateAgenda(committeeId) {
                    const committee = committeeAgendas[committeeId];
                    
                    // Update header
                    agendaHeader.querySelector('h2').textContent = committee.title;
                    agendaHeader.querySelector('p').textContent = committee.description;
                    
                    // Update agenda sections
                    agendaSections.forEach((section, index) => {
                        if (index === 0) { // Topics section
                            const topicsList = section.querySelector('.agenda-topics');
                            topicsList.innerHTML = '';
                            
                            committee.topics.forEach(topic => {
                                const li = document.createElement('li');
                                li.textContent = topic;
                                topicsList.appendChild(li);
                            });
                        } else if (index === 1) { // Schedule section
                            const scheduleList = section.querySelector('.agenda-schedule');
                            scheduleList.innerHTML = '';
                            
                            committee.schedule.forEach(item => {
                                const scheduleItem = document.createElement('div');
                                scheduleItem.classList.add('schedule-item');
                                scheduleItem.innerHTML = `
                                    <div class="schedule-time">${item.time}</div>
                                    <div class="schedule-desc">${item.desc}</div>
                                `;
                                scheduleList.appendChild(scheduleItem);
                            });
                        } else if (index === 2) { // Guide section
                            section.querySelector('.guide-content p').textContent = committee.guide;
                        }
                    });
                }
                
                // Add click event to committee items
                committeeItems.forEach(item => {
                    item.addEventListener('click', () => {
                        // Remove active class from all items
                        committeeItems.forEach(i => i.classList.remove('active'));
                        // Add active class to clicked item
                        item.classList.add('active');
                        // Update agenda
                        updateAgenda(item.dataset.committee);
                    });
                });
                
                // Initialize with first committee
                updateAgenda('unga');
            }
            
