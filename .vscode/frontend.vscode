        }

        .confirmation-container:before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 4px;
            background: linear-gradient(90deg, var(--success), #4CAF50);
        }

        .confirmation-icon {
            font-size: 80px;
            color: var(--success);
            margin-bottom: 30px;
            animation: pulse 2s infinite;
        }

        .confirmation-header {
            margin-bottom: 30px;
        }

        .confirmation-header h2 {
            font-size: 36px;
            color: var(--text);
            margin-bottom: 15px;
        }

        .confirmation-header p {
            color: var(--text-secondary);
            font-size: 18px;
            max-width: 700px;
            margin: 0 auto;
        }

        .confirmation-details {
            background: rgba(25, 30, 60, 0.5);
            border-radius: 8px;
            padding: 30px;
            margin: 40px auto;
            border: 1px solid var(--border);
            max-width: 600px;
            text-align: left;
        }

        .detail-item {
            display: flex;
            margin-bottom: 15px;
            padding-bottom: 15px;
            border-bottom: 1px dashed var(--border);
        }

        .detail-label {
            width: 180px;
            font-weight: 500;
            color: var(--text);
        }

        .detail-value {
            flex: 1;
            color: var(--text-secondary);
        }

        .confirmation-actions {
            margin-top: 40px;
        }

        @keyframes pulse {
            0% { transform: scale(1); }
            50% { transform: scale(1.1); }
            100% { transform: scale(1); }
        }

        /* Responsive adjustments */
        @media (max-width: 900px) {
            .form-grid {
                grid-template-columns: 1fr;
            }
            
            .form-group.full-width {
                grid-column: span 1;
            }
            
            .countries-grid {
                grid-template-columns: 1fr;
            }
            
            .features {
                grid-template-columns: 1fr;
            }
            
            .footer-content {
                grid-template-columns: repeat(2, 1fr);
            }
            
            .nav-container {
                gap: 40px;
            }
            
            .icon-wrapper {
                width: 100px;
                height: 100px;
            }
            
            .nav-icon {
                font-size: 40px;
            }
            
            .nav-text {
                font-size: 1.5rem;
            }
            
            .committees-container, .resources-container {
                flex-direction: column;
            }
            
            .committee-list {
                max-height: 300px;
                position: static;
            }
            
            .testimonials-container {
                flex-direction: column;
            }

            .info-grid {
                grid-template-columns: 1fr;
            }
            
            .method-info {
                margin-left: 0;
                margin-top: 20px;
            }
        }

        @media (max-width: 768px) {
            .header-content {
                flex-direction: column;
                text-align: center;
            }
            
            nav ul {
                margin-top: 20px;
                justify-content: center;
            }
            
            nav li {
                margin: 0 15px;
            }
            
            .stats {
                flex-direction: column;
                gap: 30px;
            }
            
            .hero h1 {
                font-size: 42px;
            }
            
            .hero p {
                font-size: 18px;
            }

            .payment-container, .confirmation-container {
                padding: 30px 20px;
            }
            
            .gpay-id {
                font-size: 18px;
            }
        }

        @media (max-width: 650px) {
            .nav-container {
                flex-direction: column;
                gap: 50px;
                margin-top: 60px;
            }
            
            .icon-wrapper {
                width: 90px;
