        .step-label {
            font-size: 14px;
            color: var(--text-secondary);
        }

        .payment-info {
            background: rgba(25, 30, 60, 0.5);
            border-radius: 8px;
            padding: 30px;
            margin-bottom: 30px;
            border: 1px solid var(--border);
