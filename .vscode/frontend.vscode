        .upload-box:hover {
            border-color: var(--accent-light);
            background: rgba(41, 47, 85, 0.5);
        }

        .upload-icon {
            font-size: 50px;
            color: var(--accent-light);
            margin-bottom: 20px;
        }

        .upload-text {
