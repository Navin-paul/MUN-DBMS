        .note-icon {
            color: var(--gold);
            margin-right: 10px;
        }

        /* Confirmation Page */
        .confirmation-container {
            background: var(--primary);
            border-radius: 12px;
            padding: 50px;
            margin: 40px auto;
            border: 1px solid var(--border);
            box-shadow: 0 10px 40px rgba(0, 0, 0, 0.3);
            max-width: 900px;
