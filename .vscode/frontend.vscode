            color: var(--text-secondary);
            margin-bottom: 8px;
        }

        .info-value {
            font-size: 18px;
            font-weight: 500;
        }

        .payment-amount {
            text-align: center;
            padding: 20px;
            background: rgba(92, 107, 192, 0.1);
            border-radius: 8px;
            border: 1px dashed var(--accent-light);
            margin-bottom: 30px;
        }

        .amount-label {
            font-size: 16px;
            color: var(--text-secondary);
            margin-bottom: 10px;
        }

        .amount-value {
            font-size: 42px;
            font-family: 'Orbitron', sans-serif;
            color: var(--star-yellow);
        }

        .payment-method {
            background: rgba(25, 30, 60, 0.5);
            border-radius: 8px;
            padding: 30px;
            margin-bottom: 30px;
            border: 1px solid var(--border);
        }

        .method-header {
            display: flex;
            align-items: center;
            margin-bottom: 20px;
        }

        .method-icon {
            width: 60px;
            height: 60px;
            background: var(--secondary);
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 28px;
            margin-right: 20px;
            color: var(--accent-light);
        }

        .method-title {
            font-size: 22px;
            font-weight: 600;
        }

        .method-info {
            margin-left: 80px;
        }

        .method-desc {
            color: var(--text-secondary);
            margin-bottom: 15px;
        }

        .gpay-id {
            background: rgba(255, 255, 255, 0.1);
